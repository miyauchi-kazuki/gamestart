<div class="user-index-wrapper">
  <div class="container users-new-container">
    <%= @users.each do |user| %>
     <div class="row">
       <div class="col-md-6 col-md-offset-3">
         <div class="user-index-single">
          <h2 class="user-index-author">
           <%= user.name %>
           <%= user.gametitle %>
          </h2>
         </div>
       </div>
     </div>
    <% end %>
  </div>
</div>  